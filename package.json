{
  "name": "configuration",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"**/*.ts\"",
    "build:client": "cd client && npm run build",
    "build:server": "tsc -p ./tsconfig.json",
    "start:client": "cd client && npm start",
    "start:server": "nodemon --exec ts-node -r dotenv/config -r tsconfig-paths/register dist/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run lint && npm run start:server\" \"cd client && npm start\"",
    "start": "concurrently \"node --exec ts-node -r dotenv/config -r tsconfig-paths/register dist/index.js\" \"cd client && npm start\"",
    "build": "npm run build:server && npm run build:client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "seed": "ts-node -r dotenv/config -r tsconfig-paths/register server/src/config/seed.config.ts",
    "seed:clean": "npm run seed clean",
    "seed:users": "npm run seed users",
    "seed:emails": "npm run seed emails",
    "seed:users:clean": "npm run seed clean -- users"
  },
  "keywords": [],
